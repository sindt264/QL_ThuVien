@model QL_ThuVien.Models.TaiLieu

@{
    ViewBag.Title = "SearchSDKCB";
    Layout = "~/Views/Shared/_LayOutPhieuYeuCau.cshtml";
}

<script type="text/javascript">
    $(document).ready(function () {

        $('#TK1').click(function () {
            var cmb = document.getElementById("SoThe")
            //alert(cmb.value)
            $.ajax({
                type: "GET",
                url: '/PhieuYeuCaus/SearchSoThe/',
                data: { 'id': cmb.value },
                success: function (result) {
                    $('#ViewST').html(result)
                }
            })
        })


        $('#TK2').click(function () {
            var cmb = document.getElementById("SDKCB")
            //alert(cmb.value)
            $.ajax({
                type: "GET",
                url: '/PhieuYeuCaus/SearchSDKCB/',
                data: { 'id': cmb.value },
                success: function (result) {
                    $('#ViewSDKCB').html(result)
                }
            })
        })


    })

</script>
